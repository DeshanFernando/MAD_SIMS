<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deshan.mad_sims">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Attendance.desMainActivity">
            <intent-filter>
                <action android:name=".desMainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Attendance.desEnter_Attendance">
            <intent-filter>
                <action android:name=".desEnter_Attendance" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Attendance.desDisplay">
            <intent-filter>
                <action android:name=".desDisplay" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Attendance.desPercentage">
            <intent-filter>
                <action android:name=".desPercentage" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Attendance.desUpdate">
            <intent-filter>
                <action android:name=".desUpdate" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MAD_MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Events.PumMainActivity" />
        <activity android:name=".Events.PumCreate" />
        <activity android:name=".Events.PumUpdate" />
        <activity android:name=".Events.PumView" />
        <activity android:name=".Events.PumDelete"></activity>
      
        <activity android:name=".diary.DiaryActivity"></activity>

    </application>

</manifest>